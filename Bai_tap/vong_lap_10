a = int(input("Nhập số thứ nhất: "))
b = int(input("Nhập số thứ hai: "))
def gcd(a, b):
    min_val = min(a, b)
    ucln = 1  
    for i in range(1, min_val + 1):
        if a % i == 0 and b % i == 0:
            ucln = i 
    return ucln
ucln = gcd(a, b)
print(f"Ước chung lớn nhất của {a} và {b} là: {ucln}")

