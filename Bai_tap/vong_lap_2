n = int(input("Nhập giá trị n: "))
S1 = 0
for i in range(1, n + 1):
    S1 += i
print("S1 =", S1)
S2 = 1
for i in range(1, n):
    S2 *= i
print("S2 =", S2)
S3 = 0
for i in range(1, n + 1):
    S3 += ((-1) ** i) / i
print("S3 =", S3)
S4 = 0
for k in range(n + 1):
    S4 += k / (k + 2)
print("S4 =", S4)
